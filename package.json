{
  "name": "install-cargo-prebuilt-action",
  "version": "3.1.3",
  "private": true,
  "description": "Install cargo prebuilt to a runner.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cargo-prebuilt/cargo-prebuilt-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "harmless-tech, cargo-prebuilt",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.11.20",
    "@typescript-eslint/parser": "^7.0.2",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
